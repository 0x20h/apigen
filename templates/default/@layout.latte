<!DOCTYPE html>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
	<meta name="generator" content="{$generator} {$version}">
	<meta name="robots" content="noindex" n:if="'source' === $active">

	<title>{include #title}{if 'overview' !== $active && $config->title} | {$config->title}{/if}</title>

	{var combined_js => 'resources/combined.js'}
	<script type="text/javascript" src="{$combined_js|staticFile}"></script>
	{var allclasses_js => 'allclasses.js'}
	<script n:if="$config->googleCse" type="text/javascript" src="{$allclasses_js|staticFile}"></script>
	{var style_css => 'resources/style.css'}
	<link rel="stylesheet" type="text/css" media="all" href="{$style_css|staticFile}">
	<link n:if="$config->googleCse" rel="search" type="application/opensearchdescription+xml" title="{$config->title}" href="{$config->baseUrl}/opensearch.xml">

	<script n:if="$config->googleAnalytics" type="text/javascript">
		var _gaq = _gaq || [];
		_gaq.push(['_setAccount', {$config->googleAnalytics}]);
		_gaq.push(['_trackPageview']);

		(function() {
			var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
			ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
			var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
		})();
	</script>
</head>

<body>
<div id="main">
<div id="leftWrapper">
	<div id="left">
		<a href="index.html">Overview</a>

		{if $packages}
		<h3>Packages</h3>
		<ul>
			{foreach $packages as $item}
			<li n:class="$package === $item || 0 === strpos($package, $item . '\\') ? active"><a href="{$item|packageUrl}">{$template->subpackageName($item) ?: $item}</a>
				{if substr_count($iterator->nextValue, '\\') > substr_count($item, '\\')}
					<ul>
				{else}
					</li>
					{if substr_count($iterator->nextValue, '\\') < substr_count($item, '\\')}
						{!=str_repeat('</ul></li>', substr_count($item, '\\') - substr_count($iterator->nextValue, '\\'))}
					{/if}
				{/if}
			{/foreach}
		</ul>
		{/if}

		{if $namespaces}
		<h3>Namespaces</h3>
		<ul>
			{foreach $namespaces as $item}
			<li n:class="$namespace === $item || 0 === strpos($namespace, $item . '\\') ? active"><a href="{$item|namespaceUrl}">{$item}</a>
				{if substr_count($iterator->nextValue, '\\') > substr_count($item, '\\')}
					<ul>
				{else}
					</li>
					{if substr_count($iterator->nextValue, '\\') < substr_count($item, '\\')}
						{!=str_repeat('</ul></li>', substr_count($item, '\\') - substr_count($iterator->nextValue, '\\'))}
					{/if}
				{/if}
			{/foreach}
		</ul>
		{/if}

		{if $packages || $namespaces}
		<hr>
		{/if}

		{if $classes}
		<h3>Classes</h3>
		<ul>
		{foreach $classes as $item}
			<li n:class="$class === $item ? active"><a href="{$item|classUrl}">{if $namespace}{$item->shortName}{else}{$item->name}{/if}</a></li>
		{/foreach}
		</ul>
		{/if}

		{if $interfaces}
		<h3>Interfaces</h3>
		<ul>
		{foreach $interfaces as $item}
			<li n:class="$class === $item ? active"><a href="{$item|classUrl}">{if $namespace}{$item->shortName}{else}{$item->name}{/if}</a></li>
		{/foreach}
		</ul>
		{/if}

		{if $exceptions}
		<h3>Exceptions</h3>
		<ul>
		{foreach $exceptions as $item}
			<li n:class="$class === $item ? active"><a href="{$item|classUrl}">{if $namespace}{$item->shortName}{else}{$item->name}{/if}</a></li>
		{/foreach}
		</ul>
		{/if}
	</div>
</div>
<div id="rightWrapper">
	<div id="right">
		<form n:if="$config->googleCse" action="http://www.google.com/cse" id="search" onsubmit="if(this.q.value.indexOf('more:')==-1)this.q.value+=' more:api'">
			<input type="hidden" name="cx" value="{$config->googleCse}"><input type="hidden" name="ie" value="UTF-8"><input type="text" name="q" class="text"><input type="submit" value="Search">
		</form>

		<div id="navigation">
			<ul>
				<li n:class="'overview' === $active ? active">
					<a n:tag-if="'overview' !== $active" href="index.html" title="Overview"><span>Overview</span></a>
				</li>
				<li n:class="'package' === $active ? active" n:if="$packages">
					<a n:tag-if="'package' !== $active && $package" href="{$package|packageUrl}" title="Summary of {$package}"><span>Package</span></a>
				</li>
				<li n:class="'namespace' === $active ? active" n:if="$namespaces">
					<a n:tag-if="'namespace' !== $active && $namespace" href="{$namespace|namespaceUrl}" title="Summary of {$namespace}"><span>Namespace</span></a>
				</li>
				<li n:class="'class' === $active ? active">
					<a n:tag-if="'class' !== $active && $class" href="{$class|classUrl}" title="Summary of {$class->name}"><span>Class</span></a>
				</li>
			</ul>
			<ul>
				<li n:class="'tree' === $active ? active" n:if="$config->tree">
					<a n:tag-if="'tree' !== $active" href="tree.html" title="Tree view of classes, interfaces and exceptions"><span>Tree</span></a>
				</li>
				<li n:class="'deprecated' === $active ? active" n:if="$config->deprecated">
					<a n:tag-if="'deprecated' !== $active" href="deprecated.html" title="List of deprecated elements"><span>Deprecated</span></a>
				</li>
				<li n:class="'todo' === $active ? active" n:if="$config->todo">
					<a n:tag-if="'todo' !== $active" href="todo.html" title="Todo list"><span>Todo</span></a>
				</li>
			</ul>
		</div>

		{include #content}

		<div id="footer">
			{$config->title} API documentation generated by <a href="http://github.com/Andrewsville/apigen">{$generator}</a>.<br>
			Generated using the <a href="https://github.com/Andrewsville/PHP-Token-Reflection" title="All your tokens are belong to us">TokenReflection</a> library.
		</div>
	</div>
</div>
</div>
</body>
</html>
