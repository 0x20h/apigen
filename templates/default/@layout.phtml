 * @param  string $fileRoot
 *}
{default $class = NULL, $namespace = NULL, package = NULL}
<!DOCTYPE html>
<html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
	<meta name="generator" content="Nette Framework Apigen">
	<meta name="robots" content="noindex" n:ifset="$noindex">

	<title>{$title} | {block title}{$title}{/block}</title>

	<script type="text/javascript" src="media/combined.js"></script>
	<script type="text/javascript" src="allclasses.js"></script>
	<link rel="stylesheet" type="text/css" media="all" href="media/style.css?v={=date('YmdH')}">
	<link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
</head>

<body>
<div id="main">
<div id="leftWrapper"
	<div id="left">
		<a href="allclasses.html">All Classes</a>

		{if $packages}
		<h3>Packages</h3>
		<ul>
		{if $package}
			<li class="all"><a href="overview.html#packages">All</a></li>
		{/if}
		{foreach $packages as $item}
			<li><a href="{$item|packageLink}" {if $package === $item}class="active"{/if}>{$item ? $item : 'None'}</a></li>
		{/foreach}
		</ul>
		{/if}

		{if $namespaces}
		<h3>Namespaces</h3>
		<ul>
		{if $namespace}
			<li class="all"><a href="overview.html#namespaces">All</a></li>
		{/if}
		{foreach $namespaces as $item}
			<li><a href="{$item|namespaceLink}" {if $namespace === $item}class="active"{/if}>{$item ? $item : 'None'}</a></li>
		{/foreach}
		</ul>
		{/if}

		{if $classes}
		<h3>Classes</h3>

		<ul>
		{if $class}
			<li class="all"><a href="allclasses.html#classes">All</a></li>
			{if $package}
				<li class="all"><a href="{$package|packageLink}#classes">All in package {$package}</a></li>
			{/if}
			{if $namespace}
				<li class="all"><a href="{$namespace|namespaceLink}#classes">All in namespace {$namespace}</a></li>
			{/if}
		{/if}
		{foreach $classes as $item}
			<li><a href="{$item|classLink}"{if $class === $item} class="active"{/if}>{if $namespace}{$item->shortName}{else}{$item->name}{/if}</a></li>
		{/foreach}
		</ul>
		{/if}

		{if $interfaces}
		<h3>Interfaces</h3>

		<ul>
		{if $class}
			<li class="all"><a href="allclasses.html#interfaces">All</a></li>
			{if $package}
				<li class="all"><a href="{$package|packageLink}#interfaces">All in package {$package}</a></li>
			{/if}
			{if $namespace}
				<li class="all"><a href="{$namespace|namespaceLink}#interfaces">All in namespace {$namespace}</a></li>
			{/if}
		{/if}
		{foreach $interfaces as $item}
			<li><a href="{$item|classLink}"{if $class === $item} class="active"{/if}>{if $namespace}{$item->shortName}{else}{$item->name}{/if}</a></li>
		{/foreach}
		</ul>
		{/if}
	</div>
</div>
<div id="rightWrapper">
	<div id="right">
		<form action="http://www.google.com/cse" id="search" onsubmit="if(this.q.value.indexOf('more:')==-1)this.q.value+=' more:api'">
			<input type="hidden" name="cx" value="011549293477758430224:vxofa9ufnhc"><input type="hidden" name="ie" value="UTF-8"><input type="text" name="q" class="text"><input type="submit" value="Search">
		</form>

		{include #content}

		<div id="footer">
			{$title} generated by <a href="http://github.com/nette/apigen">Apigen</a>
		</div>
	</div>
</div>
</div>
</body>
</html>
